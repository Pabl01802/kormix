{"ast":null,"code":"import { uuid } from \"vue-uuid\";\nimport { gsap } from 'gsap';\nimport { ref, onMounted } from 'vue';\nexport default {\n  name: \"CarouselGallery\",\n  setup() {\n    onMounted(() => {\n      console.log(ref1.value);\n    });\n    const ref1 = ref(null);\n    const areas = [{\n      id: uuid.v1(),\n      title: 'GMINA KÓRNIK',\n      src: 'https://kormix.pl/wp-content/uploads/2024/02/gminaKorniknastroneofficial-700x800.png'\n    }, {\n      id: uuid.v1(),\n      title: 'GMINA MOSINA',\n      src: 'https://kormix.pl/wp-content/uploads/2024/02/mosinanastrone-706x706.png'\n    }, {\n      id: uuid.v1(),\n      title: 'GMINA BRODNICA',\n      src: 'https://kormix.pl/wp-content/uploads/2024/02/brodnicanastrone-722x722.png'\n    }, {\n      id: uuid.v1(),\n      title: 'MIASTO POZNAŃ',\n      src: 'https://kormix.pl/wp-content/uploads/2024/02/poznannastrone1-722x722.png'\n    }, {\n      id: uuid.v1(),\n      title: 'MIASTO PUSZCZYKOWO',\n      src: 'https://kormix.pl/wp-content/uploads/2024/02/puszczykowonastrone-718x718.png'\n    }];\n    const test = el => {\n      gsap.to(el, {\n        scale: 2,\n        duration: 5\n      });\n    };\n    const test2 = el => {\n      console.log('git 2' + el);\n    };\n    return {\n      test,\n      test2,\n      areas,\n      ref1\n    };\n  }\n};","map":{"version":3,"names":["uuid","gsap","ref","onMounted","name","setup","console","log","ref1","value","areas","id","v1","title","src","test","el","to","scale","duration","test2"],"sources":["C:\\Users\\kacpe\\Desktop\\Praca\\projects\\kormix\\src\\components\\CarouselGallery.vue"],"sourcesContent":["<script>\r\nimport { uuid } from \"vue-uuid\";\r\nimport { gsap } from 'gsap'\r\nimport { ref, onMounted } from 'vue'\r\n\r\nexport default {\r\n  name: \"CarouselGallery\",\r\n  setup(){\r\n    onMounted(() => {\r\n      console.log(ref1.value)\r\n    })\r\n    const ref1 = ref(null)\r\n    const areas = [\r\n      {\r\n        id: uuid.v1(),\r\n        title: 'GMINA KÓRNIK',\r\n        src: 'https://kormix.pl/wp-content/uploads/2024/02/gminaKorniknastroneofficial-700x800.png'\r\n      },\r\n      {\r\n        id: uuid.v1(),\r\n        title: 'GMINA MOSINA',\r\n        src: 'https://kormix.pl/wp-content/uploads/2024/02/mosinanastrone-706x706.png'\r\n      },\r\n      {\r\n        id: uuid.v1(),\r\n        title: 'GMINA BRODNICA',\r\n        src: 'https://kormix.pl/wp-content/uploads/2024/02/brodnicanastrone-722x722.png'\r\n      },\r\n      {\r\n        id: uuid.v1(),\r\n        title: 'MIASTO POZNAŃ',\r\n        src: 'https://kormix.pl/wp-content/uploads/2024/02/poznannastrone1-722x722.png'\r\n      },\r\n      {\r\n        id: uuid.v1(),\r\n        title: 'MIASTO PUSZCZYKOWO',\r\n        src: 'https://kormix.pl/wp-content/uploads/2024/02/puszczykowonastrone-718x718.png'\r\n      }\r\n    ]\r\n    const test = el => {\r\n      gsap.to(el, {\r\n        scale: 2,\r\n        duration: 5\r\n      })\r\n    }\r\n    const test2 = el => {\r\n      console.log('git 2' + el)\r\n    }\r\n    return { test, test2, areas, ref1 }\r\n  }\r\n}\r\n</script>\r\n\r\n<template>\r\n  <div>\r\n    <div class=\"images-wrapper\">\r\n<!--      <transition-group appear @before-enter=\"test\" @enter=\"test2\">-->\r\n      <div :ref=\"ref1\" :class=\"'area-' + index\" v-for=\"(area, index) in areas\" v-bind:key=\"area.id\">\r\n        <h4>{{ area.title }}</h4>\r\n        <img :alt=\"area.title\" :src=\"area.src\">\r\n      </div>\r\n<!--      </transition-group>-->\r\n    </div>\r\n    <div class=\"carousel-current\" onload=\"test\">\r\n      <div :class=\"'circle-' + index\" v-for=\"(item, index) in areas.length\" v-bind:key=\"index\" />\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<style scoped>\r\n  .images-wrapper{\r\n    width: 100%;\r\n    overflow: hidden;\r\n    display: flex;\r\n  }\r\n  div[class^='area']{\r\n    min-width: 100%;\r\n    display: flex;\r\n    justify-content: space-around;\r\n    align-items: center;\r\n  }\r\n  div[class^='area']:nth-child(odd){\r\n    flex-direction: row-reverse;\r\n  }\r\n  img{\r\n    width: 500px;\r\n  }\r\n  h4{\r\n    margin: 0;\r\n    font-weight: 400;\r\n    width: 200px;\r\n    text-align: left;\r\n    font-size: 48px;\r\n  }\r\n  .carousel-current{\r\n    display: flex;\r\n    gap: 10px;\r\n    justify-content: center;\r\n  }\r\n  div[class^='circle']{\r\n    width: 10px;\r\n    height: 10px;\r\n    background: var(--text-light);\r\n    border-radius: 50%;\r\n    cursor: pointer;\r\n  }\r\n</style>"],"mappings":"AACA,SAASA,IAAG,QAAS,UAAU;AAC/B,SAASC,IAAG,QAAS,MAAK;AAC1B,SAASC,GAAG,EAAEC,SAAQ,QAAS,KAAI;AAEnC,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,KAAKA,CAAA,EAAE;IACLF,SAAS,CAAC,MAAM;MACdG,OAAO,CAACC,GAAG,CAACC,IAAI,CAACC,KAAK;IACxB,CAAC;IACD,MAAMD,IAAG,GAAIN,GAAG,CAAC,IAAI;IACrB,MAAMQ,KAAI,GAAI,CACZ;MACEC,EAAE,EAAEX,IAAI,CAACY,EAAE,CAAC,CAAC;MACbC,KAAK,EAAE,cAAc;MACrBC,GAAG,EAAE;IACP,CAAC,EACD;MACEH,EAAE,EAAEX,IAAI,CAACY,EAAE,CAAC,CAAC;MACbC,KAAK,EAAE,cAAc;MACrBC,GAAG,EAAE;IACP,CAAC,EACD;MACEH,EAAE,EAAEX,IAAI,CAACY,EAAE,CAAC,CAAC;MACbC,KAAK,EAAE,gBAAgB;MACvBC,GAAG,EAAE;IACP,CAAC,EACD;MACEH,EAAE,EAAEX,IAAI,CAACY,EAAE,CAAC,CAAC;MACbC,KAAK,EAAE,eAAe;MACtBC,GAAG,EAAE;IACP,CAAC,EACD;MACEH,EAAE,EAAEX,IAAI,CAACY,EAAE,CAAC,CAAC;MACbC,KAAK,EAAE,oBAAoB;MAC3BC,GAAG,EAAE;IACP,EACF;IACA,MAAMC,IAAG,GAAIC,EAAC,IAAK;MACjBf,IAAI,CAACgB,EAAE,CAACD,EAAE,EAAE;QACVE,KAAK,EAAE,CAAC;QACRC,QAAQ,EAAE;MACZ,CAAC;IACH;IACA,MAAMC,KAAI,GAAIJ,EAAC,IAAK;MAClBV,OAAO,CAACC,GAAG,CAAC,OAAM,GAAIS,EAAE;IAC1B;IACA,OAAO;MAAED,IAAI;MAAEK,KAAK;MAAEV,KAAK;MAAEF;IAAK;EACpC;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}
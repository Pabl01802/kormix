{"ast":null,"code":"import { uuid } from \"vue-uuid\";\nimport { gsap } from 'gsap';\nimport { ScrollTrigger } from \"gsap/ScrollTrigger\";\nimport { Draggable } from \"gsap/Draggable\";\nimport { ScrollToPlugin } from 'gsap/ScrollToPlugin';\nimport { ref, onMounted } from 'vue';\nexport default {\n  name: \"CarouselGallery\",\n  setup() {\n    let current = 0;\n    const imgRef = ref();\n    const carouselRef = ref();\n    onMounted(() => {\n      gsap.registerPlugin(ScrollTrigger, Draggable, ScrollToPlugin);\n      window.addEventListener('resize', () => {\n        console.log(ScrollTrigger.isInViewport(imgRef.value[current], 1, true));\n      });\n    });\n    return {\n      imgRef,\n      carouselRef,\n      current\n    };\n  },\n  data() {\n    const areas = [{\n      id: uuid.v1(),\n      title: 'GMINA KÓRNIK',\n      src: 'https://kormix.pl/wp-content/uploads/2024/02/gminaKorniknastroneofficial-700x800.png'\n    }, {\n      id: uuid.v1(),\n      title: 'GMINA MOSINA',\n      src: 'https://kormix.pl/wp-content/uploads/2024/02/mosinanastrone-706x706.png'\n    }, {\n      id: uuid.v1(),\n      title: 'GMINA BRODNICA',\n      src: 'https://kormix.pl/wp-content/uploads/2024/02/brodnicanastrone-722x722.png'\n    }, {\n      id: uuid.v1(),\n      title: 'MIASTO POZNAŃ',\n      src: 'https://kormix.pl/wp-content/uploads/2024/02/poznannastrone1-722x722.png'\n    }, {\n      id: uuid.v1(),\n      title: 'MIASTO PUSZCZYKOWO',\n      src: 'https://kormix.pl/wp-content/uploads/2024/02/puszczykowonastrone-718x718.png'\n    }];\n    return {\n      areas\n    };\n  },\n  methods: {\n    scrollToElement(index) {\n      gsap.to(this.carouselRef, {\n        scrollTo: {\n          x: this.imgRef[index]\n        },\n        duration: 1\n      });\n    },\n    changeImg(direction) {\n      if ((this.current === this.areas.length - 1 || 0) && direction === 'prev' || 'next') {\n        this.current = 0;\n        this.scrollToElement(0);\n      } else {\n        if (direction === 'next') {\n          this.current++;\n          this.scrollToElement(this.current);\n        } else if (direction === 'prev') {\n          this.current--;\n          this.scrollToElement(this.current);\n        } else {\n          this.current = direction;\n          this.scrollToElement(direction);\n        }\n      }\n    }\n  }\n};","map":{"version":3,"names":["uuid","gsap","ScrollTrigger","Draggable","ScrollToPlugin","ref","onMounted","name","setup","current","imgRef","carouselRef","registerPlugin","window","addEventListener","console","log","isInViewport","value","data","areas","id","v1","title","src","methods","scrollToElement","index","to","scrollTo","x","duration","changeImg","direction","length"],"sources":["C:\\Users\\kacpe\\Desktop\\Praca\\projects\\kormix\\src\\components\\CarouselGallery.vue"],"sourcesContent":["<script>\r\nimport { uuid } from \"vue-uuid\";\r\nimport { gsap } from 'gsap'\r\nimport { ScrollTrigger } from \"gsap/ScrollTrigger\";\r\nimport { Draggable } from \"gsap/Draggable\";\r\nimport { ScrollToPlugin } from 'gsap/ScrollToPlugin'\r\nimport { ref, onMounted } from 'vue'\r\n\r\nexport default {\r\n  name: \"CarouselGallery\",\r\n  setup(){\r\n    let current = 0\r\n    const imgRef = ref()\r\n    const carouselRef = ref()\r\n    onMounted(() => {\r\n      gsap.registerPlugin(ScrollTrigger, Draggable, ScrollToPlugin)\r\n      window.addEventListener('resize', () => {\r\n        console.log(ScrollTrigger.isInViewport(imgRef.value[current], 1, true))\r\n      })\r\n    })\r\n    return { imgRef, carouselRef, current }\r\n  },\r\n  data(){\r\n    const areas = [\r\n      {\r\n        id: uuid.v1(),\r\n        title: 'GMINA KÓRNIK',\r\n        src: 'https://kormix.pl/wp-content/uploads/2024/02/gminaKorniknastroneofficial-700x800.png'\r\n      },\r\n      {\r\n        id: uuid.v1(),\r\n        title: 'GMINA MOSINA',\r\n        src: 'https://kormix.pl/wp-content/uploads/2024/02/mosinanastrone-706x706.png'\r\n      },\r\n      {\r\n        id: uuid.v1(),\r\n        title: 'GMINA BRODNICA',\r\n        src: 'https://kormix.pl/wp-content/uploads/2024/02/brodnicanastrone-722x722.png'\r\n      },\r\n      {\r\n        id: uuid.v1(),\r\n        title: 'MIASTO POZNAŃ',\r\n        src: 'https://kormix.pl/wp-content/uploads/2024/02/poznannastrone1-722x722.png'\r\n      },\r\n      {\r\n        id: uuid.v1(),\r\n        title: 'MIASTO PUSZCZYKOWO',\r\n        src: 'https://kormix.pl/wp-content/uploads/2024/02/puszczykowonastrone-718x718.png'\r\n      }\r\n    ]\r\n    return { areas }\r\n  },\r\n  methods: {\r\n    scrollToElement(index){\r\n      gsap.to(this.carouselRef, {\r\n        scrollTo: {\r\n          x: this.imgRef[index]\r\n        },\r\n        duration: 1,\r\n      })\r\n    },\r\n    changeImg(direction){\r\n      if((this.current === this.areas.length - 1 || 0) && direction === 'prev' || 'next'){\r\n        this.current = 0\r\n        this.scrollToElement(0)\r\n      }else{\r\n        if(direction === 'next'){\r\n          this.current++\r\n          this.scrollToElement(this.current)\r\n        }\r\n        else if(direction === 'prev'){\r\n          this.current--\r\n          this.scrollToElement(this.current)\r\n        }\r\n        else{\r\n          this.current = direction\r\n          this.scrollToElement(direction)\r\n        }\r\n      }\r\n    },\r\n  },\r\n}\r\n</script>\r\n\r\n<template>\r\n  <div class=\"container\">\r\n    <div ref=\"carouselRef\" class=\"images-wrapper\">\r\n      <button class=\"change-img\" @click=\"changeImg('prev')\" />\r\n      <div ref=\"imgRef\" :class=\"'area-' + index\" v-for=\"(area, index) in areas\" v-bind:key=\"area.id\">\r\n        <h4>{{ area.title }}</h4>\r\n        <img :alt=\"area.title\" :src=\"area.src\">\r\n      </div>\r\n      <button class=\"change-img\" @click=\"changeImg('next')\" />\r\n    </div>\r\n    <div class=\"carousel-current\">\r\n      <div class=\"circle\" :class=\"[this.current === index && 'active']\" @click=\"changeImg(index)\" v-for=\"(item, index) in areas.length\" v-bind:key=\"index\" />\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<style scoped>\r\n  .container{\r\n    position: relative;\r\n  }\r\n  .images-wrapper{\r\n    width: 100%;\r\n    overflow: hidden;\r\n    display: flex;\r\n    background: red;\r\n  }\r\n  div[class^='area']{\r\n    min-width: 100%;\r\n    display: flex;\r\n    justify-content: space-around;\r\n    align-items: center;\r\n  }\r\n  div[class^='area']:nth-child(odd){\r\n    flex-direction: row-reverse;\r\n  }\r\n  img{\r\n    width: 500px;\r\n  }\r\n  h4{\r\n    margin: 0;\r\n    font-weight: 400;\r\n    width: 200px;\r\n    text-align: left;\r\n    font-size: 48px;\r\n  }\r\n  .carousel-current{\r\n    display: flex;\r\n    gap: 10px;\r\n    justify-content: center;\r\n  }\r\n  .circle{\r\n    width: 10px;\r\n    height: 10px;\r\n    background: var(--text-light);\r\n    border-radius: 50%;\r\n    cursor: pointer;\r\n  }\r\n  .active{\r\n    background: black;\r\n  }\r\n  .change-img{\r\n    height: 30px;\r\n    top: 50%;\r\n    transform: translateY(-50%);\r\n    appearance: none;\r\n    border: none;\r\n    background: black;\r\n  }\r\n  .change-img:hover{\r\n    cursor: pointer;\r\n  }\r\n  .change-img:first-child{\r\n    position: absolute;\r\n    left: 0;\r\n  }\r\n  .change-img:last-child{\r\n    position: absolute;\r\n    right: 0;\r\n  }\r\n</style>"],"mappings":"AACA,SAASA,IAAG,QAAS,UAAU;AAC/B,SAASC,IAAG,QAAS,MAAK;AAC1B,SAASC,aAAY,QAAS,oBAAoB;AAClD,SAASC,SAAQ,QAAS,gBAAgB;AAC1C,SAASC,cAAa,QAAS,qBAAoB;AACnD,SAASC,GAAG,EAAEC,SAAQ,QAAS,KAAI;AAEnC,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,KAAKA,CAAA,EAAE;IACL,IAAIC,OAAM,GAAI;IACd,MAAMC,MAAK,GAAIL,GAAG,CAAC;IACnB,MAAMM,WAAU,GAAIN,GAAG,CAAC;IACxBC,SAAS,CAAC,MAAM;MACdL,IAAI,CAACW,cAAc,CAACV,aAAa,EAAEC,SAAS,EAAEC,cAAc;MAC5DS,MAAM,CAACC,gBAAgB,CAAC,QAAQ,EAAE,MAAM;QACtCC,OAAO,CAACC,GAAG,CAACd,aAAa,CAACe,YAAY,CAACP,MAAM,CAACQ,KAAK,CAACT,OAAO,CAAC,EAAE,CAAC,EAAE,IAAI,CAAC;MACxE,CAAC;IACH,CAAC;IACD,OAAO;MAAEC,MAAM;MAA
om \"gsap/ScrollTrigger\";\r\nimport { Draggable } from \"gsap/Draggable\";\r\nimport { ref, onMounted } from 'vue'\r\n\r\nexport default {\r\n  name: \"CarouselGallery\",\r\n  setup(){\r\n    const imgRef = ref()\r\n    onMounted(() => {\r\n      gsap.registerPlugin(ScrollTrigger, Draggable)\r\n      console.log(ScrollTrigger.isInViewport(imgRef.value[0], 1, true))\r\n    })\r\n    // const test = el => {\r\n    //   gsap.to(el, {\r\n    //     scale: 2,\r\n    //     duration: 5\r\n    //   })\r\n    // }\r\n    // const test2 = el => {\r\n    //   console.log('git 2' + el)\r\n    // }\r\n    return { imgRef }\r\n  },\r\n  data(){\r\n    const areas = [\r\n      {\r\n        id: uuid.v1(),\r\n        title: 'GMINA KÓRNIK',\r\n        src: 'https://kormix.pl/wp-content/uploads/2024/02/gminaKorniknastroneofficial-700x800.png'\r\n      },\r\n      {\r\n        id: uuid.v1(),\r\n        title: 'GMINA MOSINA',\r\n        src: 'https://kormix.pl/wp-content/uploads/2024/02/mosinanastrone-706x706.png'\r\n      },\r\n      {\r\n        id: uuid.v1(),\r\n        title: 'GMINA BRODNICA',\r\n        src: 'https://kormix.pl/wp-content/uploads/2024/02/brodnicanastrone-722x722.png'\r\n      },\r\n      {\r\n        id: uuid.v1(),\r\n        title: 'MIASTO POZNAŃ',\r\n        src: 'https://kormix.pl/wp-content/uploads/2024/02/poznannastrone1-722x722.png'\r\n      },\r\n      {\r\n        id: uuid.v1(),\r\n        title: 'MIASTO PUSZCZYKOWO',\r\n        src: 'https://kormix.pl/wp-content/uploads/2024/02/puszczykowonastrone-718x718.png'\r\n      }\r\n    ]\r\n    return { areas }\r\n  }\r\n}\r\n</script>\r\n\r\n<template>\r\n  <div>\r\n    <div class=\"images-wrapper\">\r\n<!--      <transition-group appear @before-enter=\"test\" @enter=\"test2\">-->\r\n      <div ref=\"imgRef\" :class=\"'area-' + index\" v-for=\"(area, index) in areas\" v-bind:key=\"area.id\">\r\n        <h4>{{ area.title }}</h4>\r\n        <img :alt=\"area.title\" :src=\"area.src\">\r\n      </div>\r\n<!--      </transition-group>-->\r\n    </div>\r\n    <div class=\"carousel-current\" onload=\"test\">\r\n      <div :class=\"'circle-' + index\" v-for=\"(item, index) in areas.length\" v-bind:key=\"index\" />\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<style scoped>\r\n  .images-wrapper{\r\n    width: 100%;\r\n    overflow: hidden;\r\n    display: flex;\r\n  }\r\n  div[class^='area']{\r\n    min-width: 100%;\r\n    display: flex;\r\n    justify-content: space-around;\r\n    align-items: center;\r\n  }\r\n  div[class^='area']:nth-child(odd){\r\n    flex-direction: row-reverse;\r\n  }\r\n  img{\r\n    width: 500px;\r\n  }\r\n  h4{\r\n    margin: 0;\r\n    font-weight: 400;\r\n    width: 200px;\r\n    text-align: left;\r\n    font-size: 48px;\r\n  }\r\n  .carousel-current{\r\n    display: flex;\r\n    gap: 10px;\r\n    justify-content: center;\r\n  }\r\n  div[class^='circle']{\r\n    width: 10px;\r\n    height: 10px;\r\n    background: var(--text-light);\r\n    border-radius: 50%;\r\n    cursor: pointer;\r\n  }\r\n</style>"],"mappings":"AACA,SAASA,IAAG,QAAS,UAAU;AAC/B,SAASC,IAAG,QAAS,MAAK;AAC1B,SAASC,aAAY,QAAS,oBAAoB;AAClD,SAASC,SAAQ,QAAS,gBAAgB;AAC1C,SAASC,GAAG,EAAEC,SAAQ,QAAS,KAAI;AAEnC,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,KAAKA,CAAA,EAAE;IACL,MAAMC,MAAK,GAAIJ,GAAG,CAAC;IACnBC,SAAS,CAAC,MAAM;MACdJ,IAAI,CAACQ,cAAc,CAACP,aAAa,EAAEC,SAAS;MAC5CO,OAAO,CAACC,GAAG,CAACT,aAAa,CAACU,YAAY,CAACJ,MAAM,CAACK,KAAK,CAAC,CAAC,CAAC,EAAE,CAAC,EAAE,IAAI,CAAC;IAClE,CAAC;IACD;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA,OAAO;MAAEL;IAAO;EAClB,CAAC;EACDM,IAAIA,CAAA,EAAE;IACJ,MAAMC,KAAI,GAAI,CACZ;MACEC,EAAE,EAAEhB,IAAI,CAACiB,EAAE,CAAC,CAAC;MACbC,KAAK,EAAE,cAAc;MACrBC,GAAG,EAAE;IACP,CAAC,EACD;MACEH,EAAE,EAAEhB,IAAI,CAACiB,EAAE,CAAC,CAAC;MACbC,KAAK,EAAE,cAAc;MACrBC,GAAG,EAAE;IACP,CAAC,EACD;MACEH,EAAE,EAAEhB,IAAI,CAACiB,EAAE,CAAC,CAAC;MACbC,KAAK,EAAE,gBAAgB;MACvBC,GAAG,EAAE;IACP,CAAC,EACD;MACEH,EAAE,EAAEhB,IAAI,CAACiB,EAAE,CAAC,CAAC;MACbC,KAAK,EAAE,eAAe;MACtBC,GAAG,EAAE;IACP,CAAC,EACD;MACEH,EAAE,EAA
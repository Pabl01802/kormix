 */\r\nimport {Loader} from \"@googlemaps/js-api-loader\";\r\nimport {onMounted, ref} from \"vue\";\r\n\r\nexport default {\r\n  name: \"CompanyMap\",\r\n  setup(){\r\n    const mapDiv = ref(null)\r\n    const key = 'AIzaSyB9ib6jCQ5Tm8OpItidxHPU0U1pJjkSqwM'\r\n    const position = {\r\n      lat: 52.304610666913824,\r\n      lng: 16.955011700478387\r\n    }\r\n\r\n    const loader = new Loader({ apiKey: key })\r\n\r\n    onMounted(async () => {\r\n      const { AdvancedMarkerElement } = await google.maps.importLibrary(\"marker\");\r\n      const companyMarker = new AdvancedMarkerElement({\r\n        map,\r\n        position,\r\n        title: \"Title text for the marker at lat: 37.419, lng: -122.03\",\r\n      });\r\n      await loader.load()\r\n      const map = new google.maps.Map(mapDiv.value, {\r\n        mapId: '8c5bb697c76224c2',\r\n        center: position,\r\n        zoom: 18,\r\n      })\r\n      map.setOptions({\r\n        styles: [{\r\n          featureType: 'poi.shop',\r\n          elementType: 'labels.icon',\r\n          stylers: [{\r\n            visibility: 'off'\r\n          }]\r\n        }]\r\n      })\r\n      const marker = new google.maps.Marker({ map, position })\r\n      marker.addListener(\"click\", () => {\r\n        console.log('dw')\r\n      })\r\n    })\r\n\r\n    return { mapDiv }\r\n  }\r\n}\r\n</script>\r\n\r\n<template>\r\n  <div ref=\"mapDiv\"></div>\r\n</template>\r\n\r\n<style scoped>\r\n  div{\r\n    height: 300px;\r\n    background: red;\r\n  }\r\n</style>"],"mappings":"AACA;AACA;AACA,SAAQA,MAAM,QAAO,2BAA2B;AAChD,SAAQC,SAAS,EAAEC,GAAG,QAAO,KAAK;AAElC,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,KAAKA,CAAA,EAAE;IACL,MAAMC,MAAK,GAAIH,GAAG,CAAC,IAAI;IACvB,MAAMI,GAAE,GAAI,yCAAwC;IACpD,MAAMC,QAAO,GAAI;MACfC,GAAG,EAAE,kBAAkB;MACvBC,GAAG,EAAE;IACP;IAEA,MAAMC,MAAK,GAAI,IAAIV,MAAM,CAAC;MAAEW,MAAM,EAAEL;IAAI,CAAC;IAEzCL,SAAS,CAAC,YAAY;MACpB,MAAM;QAAEW;MAAsB,IAAI,MAAMC,MAAM,CAACC,IAAI,CAACC,aAAa,CAAC,QAAQ,CAAC;MAC3E,MAAMC,aAAY,GAAI,IAAIJ,qBAAqB,CAAC;QAC
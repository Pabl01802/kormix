le: 'MIASTO PUSZCZYKOWO',\r\n        src: 'https://kormix.pl/wp-content/uploads/2024/02/puszczykowonastrone-718x718.png'\r\n      }\r\n    ]\r\n    return { areas, current }\r\n  },\r\n  methods: {\r\n    changeImg(direction){\r\n      if(this.current > this.areas.length - 1){\r\n        this.current = 0\r\n      }else{\r\n        if(direction === 'next'){\r\n          this.current++\r\n          gsap.to(this.carouselRef.value, {\r\n            scrollTo: 400,\r\n            duration: 2\r\n          })\r\n        }\r\n        else if(direction === 'prev') this.current--\r\n        else{\r\n          this.current = direction\r\n        }\r\n      }\r\n      console.log(this.current)\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<template>\r\n  <div>\r\n    <div ref=\"carouselRef\" class=\"images-wrapper\">\r\n      <button class=\"change-img\" @click=\"changeImg('prev')\" />\r\n      <div ref=\"imgRef\" :class=\"'area-' + index\" v-for=\"(area, index) in areas\" v-bind:key=\"area.id\">\r\n        <h4>{{ area.title }}</h4>\r\n        <img :alt=\"area.title\" :src=\"area.src\">\r\n      </div>\r\n      <button class=\"change-img\" @click=\"changeImg('next')\" />\r\n    </div>\r\n    <div class=\"carousel-current\">\r\n      <div :class=\"'circle-' + index\" @click=\"changeImg(index)\" v-for=\"(item, index) in areas.length\" v-bind:key=\"index\" />\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<style scoped>\r\n  .images-wrapper{\r\n    width: 100%;\r\n    overflow: hidden;\r\n    display: flex;\r\n    position: relative;\r\n  }\r\n  div[class^='area']{\r\n    min-width: 100%;\r\n    display: flex;\r\n    justify-content: space-around;\r\n    align-items: center;\r\n  }\r\n  div[class^='area']:nth-child(odd){\r\n    flex-direction: row-reverse;\r\n  }\r\n  img{\r\n    width: 500px;\r\n  }\r\n  h4{\r\n    margin: 0;\r\n    font-weight: 400;\r\n    width: 200px;\r\n    text-align: left;\r\n    font-size: 48px;\r\n  }\r\n  .carousel-current{\r\n    display: flex;\r\n    gap: 1